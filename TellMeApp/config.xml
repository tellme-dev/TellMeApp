<?xml version="1.0" encoding="utf-8"?>
<widget defaultlocale="zh-CN" id="com.hotelmee.tellmeapp" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
  <name>智慧酒店</name>
  <description>酒店类服务资讯</description>
  <author email="dev@cordova.apache.org" href="http://cordova.io">tellme dev</author>
  <content src="app.html" />
  <access origin="*" />
  <vs:features />
  <preference name="Fullscreen" value="False" />
  <preference name="SplashScreen" value="screen" />
  <preference name="AutoHideSplashScreen" value="false" />
  <preference name="SplashScreenDelay" value="4000" />
  <preference name="windows-target-version" value="8.1" />
  <plugin name="cordova-plugin-whitelist" version="1" />
  <allow-navigation href="*" />
  <allow-intent href="*" />
  <icon src="res/icons/icon-ihotel.png" />
  <platform name="android">
    <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
  </platform>
  <platform name="android">
    <splash density="land-hdpi" src="res/screens/android/screen-hdpi-landscape.png" />
    <splash density="land-ldpi" src="res/screens/android/screen-ldpi-landscape.png" />
    <splash density="land-mdpi" src="res/screens/android/screen-mdpi-landscape.png" />
    <splash density="land-xhdpi" src="res/screens/android/tellme-screen-xhdpi-landscape.png" />
    <splash density="port-hdpi" src="res/screens/android/screen-hdpi-portrait.png" />
    <splash density="port-ldpi" src="res/screens/android/screen-ldpi-portrait.png" />
    <splash density="port-mdpi" src="res/screens/android/screen-mdpi-portrait.png" />
    <splash density="port-xhdpi" src="res/screens/android/tellme-screen-xhdpi-portrait.png" />
  </platform>
  <platform name="ios">
    <splash height="480" src="res/screens/ios/screen-iphone-portrait.png" width="320" />
    <splash height="960" src="res/screens/ios/tellme-screen-iphone-portrait-2x.png" width="640" />
    <splash height="1024" src="res/screens/ios/screen-ipad-portrait.png" width="768" />
    <splash height="2048" src="res/screens/ios/tellme-screen-ipad-portrait-2x.png" width="1536" />
    <splash height="768" src="res/screens/ios/screen-ipad-landscape.png" width="1024" />
    <splash height="1536" src="res/screens/ios/tellme-screen-ipad-landscape-2x.png" width="2048" />
    <splash height="1136" src="res/screens/ios/tellme-screen-iphone-568h-2x.png" width="640" />
    <splash height="1334" src="res/screens/ios/tellme-screen-iphone-portrait-667h.png" width="750" />
    <splash height="2208" src="res/screens/ios/tellme-screen-iphone-portrait-736h.png" width="1242" />
    <splash height="1242" src="res/screens/ios/tellme-screen-iphone-landscape-736h.png" width="2208" />
  </platform>
  <platform name="windows">
    <splash height="300" src="res/screens/windows/tellme-SplashScreen.scale-100.png" width="620" />
    <splash height="1920" src="res/screens/windows/tellme-SplashScreen.scale-240.png" width="1152" />
    <splash height="1920" src="res/screens/windows/tellme-SplashScreenPhone.scale-240.png" width="1152" />
  </platform>
  <platform name="wp8">
    <splash height="800" src="res/screens/wp8/tellme-SplashScreenImage.jpg" width="480" />
  </platform>
  <vs:plugin name="cordova-plugin-camera" version="1.2.0" />
  <vs:plugin name="cordova-plugin-whitelist" version="1.0.0" />
  <vs:plugin name="cordova-plugin-splashscreen" version="2.1.0" />
  <vs:plugin name="cordova-plugin-inappbrowser" version="1.0.1" />
  <preference name="android-minSdkVersion" value="17" />
  <preference name="target-device" value="handset" />
  <vs:plugin name="cordova-plugin-file-transfer" version="1.4.0" />
  <vs:plugin name="cordova-plugin-file-transfer" version="1.4.0" />
  <vs:plugin name="cordova-plugin-file" version="3.0.0" />
  <vs:plugin name="xu.li.cordova.wechat" src="https://github.com/xu-li/cordova-plugin-wechat" version="0.9.3">
    <param name="WECHATAPPID" value="wx8d25a682873339b9" />
  </vs:plugin>
  <vs:plugin name="cordova-plugin-weibosdk" src="https://github.com/iVanPan/cordova_weibo.git" version="0.3.4">
    <param name="WEIBO_APP_ID" value="3379583054" />
  </vs:plugin>
  <vs:plugin name="cordova-plugin-qqsdk" src="https://github.com/iVanPan/Cordova_QQ.git" version="0.3.9">
    <param name="QQ_APP_ID" value="1105008574" />
  </vs:plugin>
  <vs:plugin name="cordova-plugin-datepicker" src="https://github.com/VitaliiBlagodir/cordova-plugin-datepicker" version="0.9.2" />
  <preference name="KeepRunning" value="True" />
  <preference name="ShowTitle" value="True" />
  <preference name="InAppBrowserStorageEnabled" value="True" />
  <preference name="SuppressesIncrementalRendering" value="True" />
  <preference name="xwalkVersion" value="15+" />
  <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
  <preference name="xwalkMode" value="embedded" />
  <preference name="xwalkMultipleApk" value="true" />
  <vs:plugin name="cordova-plugin-crosswalk-webview" version="1.4.0" />
</widget>